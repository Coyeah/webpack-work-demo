{
  "name": "oops-project",
  "version": "1.0.0",
  "description": "simple",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "clean-dll": "rm -rf dll/*",
    "dll": "webpack --config=webpack/webpack.dll.config.js --mode development",
    "prod": "cross-env NODE_ENV=production webpack --color --config=webpack/webpack.prod.config.js",
    "dev": "cross-env NODE_ENV=development PORT=3000 webpack-dev-server --mode=development --config=webpack/webpack.dev.config.js",
    "analysis": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.config.js --analysis",
    "start": "npm run dev",
    "build": "npm run dll && npm run prod",
    "prettier": "node ./scripts/prettier.js",
    "lint-staged": "lint-staged",
    "precommit": "npm run lint-staged",
    "build:ssr": "webpack --color --config=webpack/webpack.ssr.config.js",
    "dev:ssr": "cross-env PORT=3000 nodemon server/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --ignore-path=.prettierignore"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coyeah/oops-project.git"
  },
  "keywords": [
    "react",
    "antd",
    "webpack"
  ],
  "author": "Coyeah_chen@outlook.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Coyeah/oops-project/issues"
  },
  "homepage": "https://github.com/Coyeah/oops-project#readme",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "antd": "^4.1.3",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.2.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "husky": "^4.2.5",
    "isomorphic-style-loader": "^5.1.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.1.4",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "thread-loader": "^2.1.3",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
